{
  "name": "ezdocs",
  "version": "1.0.4",
  "description": "Easily generate nested documentation using markdown",
  "bin": "cli.js",
  "repository": "https://github.com/rcpooley/EasyDocs",
  "scripts": {
    "start": "node cli.js",
    "build": "node buildReactTemplate.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "documentation",
    "easy",
    "easydocs"
  ],
  "author": "rcpooley",
  "license": "ISC",
  "dependencies": {
    "markdown": "^0.5.0",
    "rimraf": "^2.6.2"
  },
  "devDependencies": {
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "parcel-bundler": "^1.10.3",
    "postcss-nested": "^4.1.0"
  },
  "postcss": {
    "modules": false,
    "plugins": {
      "postcss-nested": {}
    }
  }
}
